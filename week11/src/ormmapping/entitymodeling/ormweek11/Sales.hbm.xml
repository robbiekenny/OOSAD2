<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entitymodeling.ormweek11.Sales" table="Sales" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="numSales" column="NumSales" type="integer" length="11" not-null="true" lazy="false"/>
		<property name="profit" column="Profit" type="double" not-null="true" lazy="false"/>
		<property name="expenses" column="Expenses" type="double" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
